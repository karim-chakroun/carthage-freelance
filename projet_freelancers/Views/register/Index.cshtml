
@{
    ViewBag.Title = "registration";
}



<div class="pricing-header px-3 py-3 pt-md-5 pb-md-4 mx-auto text-center">
    <h1 class="display-4">What are you?</h1>
    <p class="lead"></p>
</div>

<div class="container">
    <div class="card-deck mb-3 text-center">



        <div class="card mb-4 box-shadow">
            <div class="card-header">
                <h4 class="my-0 font-weight-normal">Freelancer</h4>
            </div>
            <div class="card-body">
                <h1 class="card-title pricing-card-title">Alot of missions are waiting for you! </h1>

                
                <a href="/freelancer/register" class="btn btn-lg btn-block btn btn-danger">Sign Up </a>
            </div>
        </div>
        <div class="card mb-4 box-shadow">
            <div class="card-header">
                <h4 class="my-0 font-weight-normal">Enterprise</h4>
            </div>
            <div class="card-body">
                <h1 class="card-title pricing-card-title">Many Freelancers are waiting for you! </h1>

                
                <a href="/entreprises/register" class="btn btn-lg btn-block btn-primary">Sign Up </a>
            </div>
        </div>
    </div>
</div>